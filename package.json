{
    "name": "weather_site_mobile",
    "version": "1.0.0",
    "description": "weather_site_mobile",
    "main": "bin/entry.js",
    "scripts": {
        "start": "node bin/entry.js",
        "start:supervisor": "supervisor bin/entry.js",
        "start:pm2": "pm2 start bin/entry.js --name=mobile",
        "stop:pm2": "pm2 stop mobile",
        "reload:pm2": "pm2 reload mobile",
        "build": "cd build && webpack",
        "build:watch": "cd build && webpack -w",
        "build:react": "cd build && webpack --config react.config.js",
        "wds": "cd build && webpack-dev-server --devtool eval --progress --colors --port 9527 --inline --hot --config wds.config.js --content-base ./../",
        "mock": "karma start ",
        "mocha": "mocha --compilers js:babel-core/register mock/**/*.spec.js"
    },
    "author": "yongchao.wu",
    "license": "ISC",
    "dependencies": {
        "babel-cli": "^6.22.2",
        "babel-core": "^6.22.1",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^6.2.10",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-polyfill": "^6.22.0",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-react": "^6.22.0",
        "babel-preset-stage-0": "^6.22.0",
        "babel-register": "^6.22.0",
        "css-loader": "^0.26.1",
        "css-modules-require-hook": "^4.0.5",
        "eslint": "^3.15.0",
        "eslint-config-airbnb": "^14.1.0",
        "eslint-loader": "^1.6.1",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^4.0.0",
        "eslint-plugin-react": "^6.9.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "fetch-jsonp": "^1.0.6",
        "file-loader": "^0.10.0",
        "iconv-lite": "^0.4.15",
        "isomorphic-fetch": "^2.2.1",
        "kcors": "^1.3.2",
        "koa": "^2.0.0",
        "koa-bodyparser": "^2.3.0",
        "koa-convert": "^1.2.0",
        "koa-onerror": "^3.0.1",
        "koa-router": "7",
        "koa-static": "^2.1.0",
        "memory-cache": "^0.1.6",
        "mini-logger": "^1.1.1",
